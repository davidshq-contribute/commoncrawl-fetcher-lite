{
  "numThreads":10,
  "maxRecords":-1,
  "maxFilesExtracted":10000,
  "maxFilesTruncated":-1,
  "indexPathsFile":"crawls.txt",
  "filesDirectory":"docs",
  "truncatedUrlsFile":"truncated.txt",
  "recordSelector": {
    "should": {
      "mime_detected": [
        {
          "match": "application/pdf",
          "sample": 0.8
        }
      ]
    },
    "must": {
      "status": [
        {
          "match": "200"
        }
      ]
    },
    "must_not" : {
      "status": [
        {
          "match": "300"
        }
      ]
    }
  }
}